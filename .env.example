# ==================================
# GITHUB MODELS (Primary)
# ==================================
GITHUB_TOKEN=your_github_token_here
GITHUB_TOKEN_1=your_github_token_1
GITHUB_TOKEN_2=your_github_token_2
GITHUB_TOKEN_3=your_github_token_3

# ==================================
# EXTERNAL PROVIDERS CONFIGURATION
# Models from these providers will have a prefix (ext-*)
# ==================================

# Anthropic (Claude) Provider
ANTHROPIC_ENABLED=0
ANTHROPIC_API_KEY=your_anthropic_api_key_here
# ANTHROPIC_BASE_URL=https://api.anthropic.com

# OpenAI Direct Provider (separate from GitHub Models)
EXT_OPENAI_ENABLED=0
EXT_OPENAI_API_KEY=your_openai_api_key_here
# EXT_OPENAI_BASE_URL=https://api.openai.com/v1

# Azure OpenAI Provider
AZURE_OPENAI_ENABLED=0
AZURE_OPENAI_API_KEY=your_azure_api_key_here
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com
AZURE_OPENAI_API_VERSION=2024-02-15-preview
AZURE_OPENAI_DEPLOYMENT=gpt-4

# Ollama (Local Models) Provider
OLLAMA_ENABLED=0
OLLAMA_BASE_URL=http://localhost:11434

# Google AI (Gemini) Provider
GOOGLE_AI_ENABLED=0
GOOGLE_API_KEY=your_google_api_key_here
# GOOGLE_BASE_URL=https://generativelanguage.googleapis.com/v1beta

# OpenRouter Provider
OPENROUTER_ENABLED=0
OPENROUTER_API_KEY=your_openrouter_api_key_here
# OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# LiteLLM Provider (Gateway to 100+ providers)
LITELLM_ENABLED=0
LITELLM_API_KEY=your_litellm_api_key_here
LITELLM_BASE_URL=http://localhost:4000

# xAI (Grok) Provider
XAI_ENABLED=0
XAI_API_KEY=your_xai_api_key_here
# XAI_BASE_URL=https://api.x.ai/v1

# ==================================
# SPECIAL PROVIDERS (Code-focused)
# ==================================

# GitHub Copilot Provider (MAIN - Most Important)
GITHUB_COPILOT_ENABLED=0
GITHUB_COPILOT_API_KEY=your_github_copilot_api_key_here
# GITHUB_COPILOT_BASE_URL=https://api.githubcopilot.com
# Token rotation support
GITHUB_COPILOT_TOKEN_1=your_copilot_token_1
GITHUB_COPILOT_TOKEN_2=your_copilot_token_2
GITHUB_COPILOT_TOKEN_3=your_copilot_token_3

# Claude Code Provider (Code-optimized Claude)
CLAUDE_CODE_ENABLED=0
CLAUDE_CODE_API_KEY=your_claude_code_api_key_here
# CLAUDE_CODE_BASE_URL=https://api.anthropic.com
# Token rotation support
CLAUDE_CODE_TOKEN_1=your_claude_code_token_1
CLAUDE_CODE_TOKEN_2=your_claude_code_token_2

# Cursor Agent Provider
CURSOR_AGENT_ENABLED=0
CURSOR_AGENT_API_KEY=your_cursor_agent_api_key_here
# CURSOR_AGENT_BASE_URL=https://api.cursor.sh
# Token rotation support
CURSOR_AGENT_TOKEN_1=your_cursor_token_1
CURSOR_AGENT_TOKEN_2=your_cursor_token_2

# Lead Worker Provider
LEAD_WORKER_ENABLED=0
LEAD_WORKER_API_KEY=your_lead_worker_api_key_here
# LEAD_WORKER_BASE_URL=https://api.leadworker.ai
# Token rotation support
LEAD_WORKER_TOKEN_1=your_lead_worker_token_1
LEAD_WORKER_TOKEN_2=your_lead_worker_token_2

# ==================================
# ATLAS PROVIDER (58 GitHub Models)
# Groups all GitHub Models API models
# ==================================
ATLAS_ENABLED=0
# ATLAS_BASE_URL=https://models.inference.ai.azure.com
# Uses GITHUB_TOKEN* for token rotation

# ==================================
# SERVER CONFIGURATION
# ==================================
PORT=4000
RATE_LIMIT_ENABLED=1
RATE_LIMIT_PER_MINUTE=30
UPSTREAM_CONCURRENCY_ENABLED=1
UPSTREAM_MAX_CONCURRENT=8
UPSTREAM_QUEUE_MAX=100
UPSTREAM_QUEUE_TIMEOUT_MS=45000
