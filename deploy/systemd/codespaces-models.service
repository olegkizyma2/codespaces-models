[Unit]
Description=Codespaces Models Proxy
After=network.target

[Service]
Type=simple
User=www-data
WorkingDirectory=/var/www/codespaces-models
Environment=NODE_ENV=production
Environment=PORT=3010
Environment=REDIS_URL=
ExecStart=/usr/bin/node /var/www/codespaces-models/server.js
Restart=on-failure
RestartSec=5
LimitNOFILE=65536

[Install]
WantedBy=multi-user.target
